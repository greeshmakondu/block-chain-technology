pragma solidity >=0.6.12 <0.9.0;

contract Datatypes{
  int public temperature =-10;
  uint256 public number;
  uint8 public age =25;
  string public text;

struct Person {
  bool vote;
  string name;
  uint age;
}
enum Color{Red,Blue,Green}
Color public favouritecolor;
Person public person;
constructor() {
  number=1 days;
  text="etherum,Blockchain,Solana";
  favouritecolor=Color.Green;
  person.name="Dakshan";
  person.age=18;
  person.vote=true;
   }
    function op(uint256 a,uint256 b) public pure returns (uint256[5] memory) {
      uint256[5] memory results;

      // Addition
      results[0] = a + b;

      // Subtraction
      results[1] = a - b;

      // Multiplication
      results[2] = a * b;

      // Division
     
      results[3] = a / b;

      // Modulus

      results[4] = a % b;

      return results;
  }

  function concatenateText(string memory a, string memory b) public pure returns ( string memory) { 
      return string(abi.encodePacked(a,b));
  }


}
